            <!--android:id="@+id/adapter_services_list_child_cbx_notification"-->
            <!--android:layout_width="wrap_content"-->
            <!--android:layout_height="wrap_content"-->
            <!--android:text="Notification"-->
            <!--android:paddingEnd="@dimen/dp_10"/>-->
    </LinearLayout>


</LinearLayout>